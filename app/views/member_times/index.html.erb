<h1>Listing member_times</h1>

<table>
  <tr>
    <th>Member</th>
    <th>Type</th>
    <th>Created at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @member_times.each do |member_time| %>
  <tr>
    <td><%= member_time.member_id %></td>
    <td><%= member_time.kind %></td>
    <td><%= member_time.created_at %></td>
    <td><%= link_to 'Show', member_time %></td>
    <td><%= link_to 'Edit', edit_member_time_path(member_time) %></td>
    <td><%= link_to 'Destroy', member_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Member time', new_member_time_path %>
